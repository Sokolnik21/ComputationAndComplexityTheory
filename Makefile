compile:
	g++ -std=c++11 Solver.cc Graph.cc BallPusher.cc ItemExtractor.cc DTBoard.cc Point.cc -o Solver.out

run:
	./Solver.out test

kompileOneFile:
	g++ -std=c++11 OneFileOfCode.cc -o OneFileOfCode.out

dtboard:
	g++ -std=c++11 DTBoard.cc -o DTBoard.out

itemextractor:
	g++ -std=c++11 ItemExtractor.cc DTBoard.cc Point.cc -o ItemExtractor.out

ballpusher:
	g++ -std=c++11 BallPusher.cc DTBoard.cc Point.cc -o BallPusher.out

graph:
	g++ -std=c++11 Graph.cc BallPusher.cc ItemExtractor.cc DTBoard.cc Point.cc -o Graph.out
